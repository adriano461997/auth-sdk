{
    "name": "hongayetu/auth-sdk",
    "description": "SDK para integração com o sistema de autenticação unificada da Honga Yetu",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Honga Yetu",
            "email": "dev@hongayetu.ao"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "HongaYetu\\AuthSDK\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "HongaYetu\\AuthSDK\\HongaAuthServiceProvider"
            ],
            "aliases": {
                "HongaAuth": "HongaYetu\\AuthSDK\\Facades\\HongaAuth"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
